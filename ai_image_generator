import os
import openai

openai.api_key = "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"
#change number to choose how many photos to generate per script run
num = 10


for i in range(num):

    response = openai.Image.create(
        prompt= "Insert Description Here",
        n=1,
        size="1024x1024"
    )
    image_url = response['data'][0]['url']

    import requests

    img_data = requests.get(image_url).content

    with open('picture' + str(i) + '.jpg', 'wb') as handler:
        handler.write(img_data)
